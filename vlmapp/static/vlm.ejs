<%- await include('./vlmapp/static/partial/header.ejs') %> 
    <title><%= title %> </title>



<!-- start of vlmhero -->
<h1 class="banner-title">Dev Courses</h1>
<header class="hero">
  <div class="banner">
    <div id="time">
      <div id="hour" style="--clr:#636a70;"></div>
      <div id="minutes" style="--clr:#01939d;"></div>
      <div id="seconds" style="--clr:#271e66;"></div>
      <div id="ampm">AM</div>
    </div>
    <div class="banner-btn">
        <div class="circle" id="sc" style="--clr:#271e66;"><i></i></div>
        <div class="circle circle2" id="mn" style="--clr:#01939d;"><i></i></div>
        <div class="circle circle3" id="hr" style="--clr:#636a70;"><i></i></div>
      <span style="--i:1">1</span>
      <span style="--i:2">2</span>
      <span style="--i:3">3</span>
      <span style="--i:4">4</span>
      <span style="--i:5">5</span>
      <span style="--i:6">6</span>
      <span style="--i:7">7</span>
      <span style="--i:8">8</span>
      <span style="--i:9">9</span>
      <span style="--i:10">10</span>
      <span style="--i:11">11</span>
      <span style="--i:12">12</span>
    </div>
  </div>
</header>

<!-- end of vlmhero -->
<!-- start of products -->
<section class="products">
  <div class="section-title">
    <h2>My Courses</h2>
  </div>
  <div class="products-center">

  </div>
</section>
