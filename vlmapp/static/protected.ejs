                    dropzomeele.addEventListener("drop", e=>{
                        e.preventDefault();
                        if(e.dataTransfer.files.length){
                            inputElement.files=e.dataTransfer.files;
                            vlmupdate(dropzomeele,e.dataTransfer.files[0])
                        }
                        const ti=e.dataTransfer.files[0]
