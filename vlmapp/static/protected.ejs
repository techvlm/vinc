                        dropzomeele.classList.remove("drop-zon--over")
                    })

                    function vlmupdate(dropzone,file){
                        let thumbele=dropzomeele.querySelector(".drop-zone__thumb");

                        if(dropzomeele.querySelector(".drop-zone__prompt")){
                            dropzomeele.querySelector(".drop-zone__prompt").remove()
                        }
                        if(!thumbele){
                            thumbele=document.createElement("div")
                            thumbele.classList.add("drop-zone__thumb")
                            dropzomeele.appendChild(thumbele)
                        }

                        thumbele.dataset.label=file.name

                        if(file.type.startsWith("image/")){
                            const io = new FileReader();

                            io.readAsDataURL(file)
                            io.onload=()=>{
                                //const queryParams = new URLSearchParams(window.location.search);
                                //queryParams.set('image', imageDataUrl);
                                thumbele.style.backgroundImage=`url('${io.result}')`;
                            }
                        }

